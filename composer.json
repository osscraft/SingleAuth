{
    "name": "osslab/single-auth",
    "description": "oauth2 implement framework",
    "type": "project",
    "keywords": [
        "framework",
        "oauth2"
    ],
    "homepage": "http://www.singleauth.net/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "lay",
            "email": "lay@liaiyong.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "laravel/lumen-framework": "5.7.*",
        "layliaiyong/data-parser": "~2.0.0",
        "layliaiyong/former": "~1.0.0",
        "league/oauth2-client": "^2.4",
        "league/oauth2-server": "^7.3",
        "vlucas/phpdotenv": "~2.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.laravel-china.org"
        }
    }
}
